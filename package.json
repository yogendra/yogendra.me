{
  "name": "yogendra_me",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.5.0"
  },
  "scripts": {
    "postinstall": "cd themes/tranquilpeak && npm install && npm run prod",
    "build": "npm run build:dev",
    "start": "hexo server --config _config.yml,_config.dev.yml",
    "clean": "hexo clean",
    "build-dev": "hexo --config _config.yml,_config.dev.yml generate",
    "build-prod-github": "hexo --config _config.yml,_config.github.yml generate ",
    "build-prod-firebase": "hexo --config _config.yml,_config.firebase.yml generate",
    "deploy-prod-github": "hexo --config _config.yml,_config.github.yml deploy -g --user=travisci --email=ci@yogendra.me",
    "deploy-prod-firebase": "hexo --config _config.yml,_config.firebase.yml generate ; firebase deploy",
    "deploy-prod": "npm run deploy:github ; npm run deploy:firebase",
    "build-prod": "npm run build-prod-firebase; npm run build-prof-github",
    "test": "echo 'no test' && exit 0"
  },
  "dependencies": {
    "hexo": "^3.5.0",
    "hexo-deployer-git": "^0.3.1",
    "hexo-deployer-rsync": "^0.1.3",
    "hexo-filter-plantuml": "^1.0.2",
    "hexo-generator-alias": "^0.1.3",
    "hexo-generator-archive": "^0.1.5",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.0",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-slideshare": "^0.1.1",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "hexo-tag-fontawesome": "^1.0.0"
  },
  "devDependencies": {
    "firebase-tools": "^3.17.3",
    "hexo-cli": "^1.0.4"
  }
}

{
  "name": "yogendra_me",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.4.0"
  },
  "scripts": {
    "hexo": "hexo --",
    "postinstall": "cd themes/tranquilpeak && yarn install && yarn prod",
    "build": "npm run build:dev",
    "start": "hexo server --config _config.yml,_config.dev.yml",
    "clean": "hexo clean",
    "build:dev": "hexo --config _config.yml,_config.dev.yml generate",
    "build:prod:github": "hexo --config _config.yml,_config.github.yml generate ",
    "build:prod:hosting": "hexo --config _config.yml,_config.hosting.yml generate",
    "deploy:github": "hexo --config _config.yml,_config.github.yml deploy -g --user=travisci --email=ci@yogendra.me",
    "deploy:hosting": "hexo --config _config.yml,_config.hosting.yml deploy  -g --host=${DEPLOY_HOST} --port=${DEPLOY_PORT} --user=${DEPLOY_USER} --root=${DEPLOY_PATH}",
    "deploy": "npm run deploy:github ; npm run deploy:hosting",
    "test": "echo 'no test' && exit 0"
  },
  "dependencies": {
    "hexo": "^3.4.0",
    "hexo-deployer-git": "^0.3.1",
    "hexo-deployer-rsync": "^0.1.3",
    "hexo-generator-alias": "^0.1.3",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.0",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-slideshare": "^0.1.1",
    "hexo-generator-tag": "^0.2.0",
    "hexo-local-plantuml": "^0.0.1",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "hexo-tag-fontawesome": "^1.0.0"
  }
}